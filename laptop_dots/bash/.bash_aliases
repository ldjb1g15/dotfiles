# Custom Aliases
alias i3con='cd $HOME/.config/i3 && vim config'
alias polcon='cd $HOME/.config/polybar && vim config'

alias gitdir='cd $HOME/codebase/git_dir'
alias matdir='cd $HOME/codebase/git_dir/matlab'
alias pydir='cd $HOME/codebase/git_dir/python/LCG'
alias glim='cd $HOME/codebase/git_dir/python/GLIMMA'
alias glimdata='cd $HOME/codebase/GLIMMA_data'
alias pyenv='source ~/codebase/git_dir/python/venv/bin/activate'
