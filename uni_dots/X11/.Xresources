urxvt*termName:         rxvt-unicode-256color
urxvt*scrollBar:        false
urxvt*perl-lib:         /usr/lib/urxvt/perl/
urxvt*perl-ext-common:  default,matcher
urxvt*urlLauncher:      /usr/bin/jumanji
urxvt*matcher.button:   1

Xft*dpi:                96
Xft*antialias:          true
Xft*hinting:            full

! urxvt*font: xft:Inconsolata:pixelsize=11, xft:Inconsolata\ for\ Powerline:pixelsize=13, xft:FontAwesome:pixelsize=12, xft:DejaVu\ Sans:pixelsize=13
! urxvt*font: xft:Inconsolata\ for\ Powerline:pixelsize=15, xft:FontAwesome:pixelsize=12, xft:DejaVu\ Sans:pixelsize=13
!
! fonts
#define fonts xft:Inconsolata:pixelsize=16, \
	xft:Inconsolata\ for\ Powerline:pixelsize=16, \
	xft:FontAwesome:pixelsize=12, \
	xft:DejaVu\ Sans\ Mono:pixelsize=12, \
	xft:DejaVu\ Sans\ \Mono\ for\ Powerline:pixelsize=12, \
	xft:xos4\ Terminess\ Powerline:pixelsize=12, \
	xft:Source\ Code\ Pro\ for\ Powerline:pixelsize=12, \
	xft:font-logos:pixelsize=12, \

urxvt*font: fonts
xterm*font: xft:DejaVu\ Sans\ Mono\ for\ Powerline:pixelsize=14, xft:FontAwesome:pixelsize=14

! special
*.foreground:   #dedede
*.background:   #2b2b2b
*.cursorColor:  #dedede

! black
*.color0:       #2e3436
*.color8:       #555753

! red
*.color1:       #cc0000
*.color9:       #ef2929

! green
*.color2:       #4e9a06
*.color10:      #8ae234

! yellow
*.color3:       #c4a000
*.color11:      #fce94f

! blue
*.color4:       #3465a4
*.color12:      #729fcf

! magenta
*.color5:       #75507b
*.color13:      #ad7fa8

! cyan
*.color6:       #06989a
*.color14:      #34e2e2

! white
*.color7:       #d3d7cf
*.color15:      #eeeeec
